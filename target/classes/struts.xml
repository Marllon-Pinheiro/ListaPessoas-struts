<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts  
Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">  
<struts>  

<constant name="struts.enable.DynamicMethodInvocation"
 value="false" />
 <constant name="struts.devMode" value="false" />
 <constant name="struts.custom.i18n.resources"
 value="ApplicationResources" />

<package name="br.com.ebix.action" extends="struts-default">  

<action name="cadastrar" class="br.com.ebix.action.CadastrarAction" method="cadastro">
 	<result name="success">cadastroRealizado.jsp</result>	
 	<result name="error">cadastroError.jsp</result>
 </action> 
 
</package>  
</struts>  